<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marenbo.www.example">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".RxJavaActivity"
            android:label="@string/title_activity_rx_java" />
        <activity
            android:name=".LoadBitmapActivity"
            android:label="@string/title_activity_load_bitmap" />
        <activity
            android:name=".FontActivity"
            android:hardwareAccelerated="false"
            android:label="@string/title_activity_font"
            android:theme="@style/myTheme" />
        <activity
            android:name=".magicviewpager.VPActivity"
            android:label="@string/title_activity_vp" />
        <activity
            android:name=".okhttppkg.OkHttpActivity"
            android:label="@string/title_activity_ok_http"></activity>
        <activity
            android:name=".DiyDrawableActivity"
            android:label="@string/title_activity_diy_drawable">

        </activity>
        <activity
            android:name=".PictureActivity"
            android:label="@string/title_activity_picture">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".sample.SampleActivity"
            android:label="sampleactivity">

        </activity>
    </application>

</manifest>
